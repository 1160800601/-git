onclick = function () {\n          controls.zoomFit.style.opacity = '1.0';\n          zoomScale = getScale(zoomStep + 1);\n          render(page);\n        };\n\n        controls.zoomOut.onclick = function () {\n          controls.zoomFit.style.opacity = '1.0';\n          zoomScale = getScale(zoomStep - 1);\n          render(page);\n        };\n\n        controls.zoomFit.onclick = function () {\n          controls.zoomFit.style.opacity = '0.0';\n          zoomScale = getScale(3);\n          render(page);\n        };\n\n        render(page);\n      }\n\n      function render(page) {\n        var width = Math.min(controls.shadowBox.getBoundingClientRect().width - 8, 640);\n        var scale = width / page.getViewport({\n          scale: 1.0\n        }).width * zoomScale;\n        var viewport = page.getViewport({\n          scale: scale\n        }); // Prepare canvas using PDF page dimensions\n\n        canvas.height = viewport.height;\n        canvas.width = viewport.width; // Render PDF page into canvas context\n\n        var renderContext = {\n          canvasContext: canvas.getContext('2d'),\n          viewport: viewport\n        }; // set the shadowbox height, if not already set\n\n        if (!controls.shadowBox.style.height) {\n          controls.shadowBox.style.height = controls.shadowBox.getBoundingClientRect().height;\n        }\n\n        page.render(renderContext);\n        /*\n        page.getTextContent().then(function(textContent){\n          // Create new instance of TextLayerBuilder class\n           let TextLayerBuilder = TextLayerBuilderFactory(page.renderTextLayer).TextLayerBuilder;\n          let textLayer = TextLayerBuilder({\n            textLayerDiv: text,\n            pageIndex: page.pageIndex,\n            viewport: viewport\n          });\n           // Set text-fragments\n          textLayer.setTextContent(textContent);\n           // Render text-fragments\n          textLayer.render();\n        });\n        */\n      }\n    }\n  }\n\n  function getPageSelection() {\n    // read page selection from /context/foo/?page={page}\n    var queryMatch = window.location.search.match(new RegExp('page=([^&]*)'));\n    var queryPage = queryMatch && queryMatch[1]; // read page selection from /context/foo/#page={page}\n\n    var hashMatch = window.location.hash.match(new RegExp('page=([^&]*)'));\n    var hashPage = hashMatch && hashMatch[1];\n    return queryPage || hashPage || 1;\n  }\n\n  function getScale(step) {\n    zoomStep = step;\n    zoomStep = Math.max(zoomStep, 1);\n    zoomStep = Math.min(zoomStep, zoomSteps);\n    var logScale = scaleMin + (scaleMax - scaleMin) * zoomStep / (zoomSteps - 1);\n    return Math.exp(logScale);\n  }\n\n  function getContentLength(url, callback, error) {\n    var http = new XMLHttpRequest();\n    http.open('HEAD', url);\n\n    http.onreadystatechange = function () {\n      if (http.readyState === http.HEADERS_RECEIVED) {\n        var contentLength = http.getResponseHeader('Content-Length');\n        var contentMB = lengthToMb(contentLength);\n        callback(contentMB);\n      }\n    };\n\n    http.onerror = error;\n    http.send();\n  }\n\n  function lengthToMb(bytes) {\n    var kb = bytes / 1024;\n    var mb = kb / 1024;\n    return Math.floor(mb);\n  }\n\n  initPdfJs();\n}\n\n//# sourceURL=webpack:///./src/view-page/pdf-page.js?");

